    <PackageIcon></PackageIcon>
    <PackageIconUrl />
    <ApplicationIcon>icon.ico</ApplicationIcon>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
    <Authors>Werayootk</Authors>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Remove="bin\**" />
